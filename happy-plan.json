{
    "baseUrl": "",
    "src": {
        "path": "src",
        "assets": {
            "path": "src/assets",
            "scripts": "src/assets/_scripts",
            "styles": "src/assets/_styles",
            "images": "src/assets/_images",
            "static": "src/assets",
            "fontcustom": "src/assets/_svg-to-fonts"
        },
        "jekyll": {
            "pages": "src/_pages",
            "posts": "src/_layouts",
            "partials": "src/_partials",
            "config": "src/_config/config.yml"
        },
        "medias": "src/medias",
        "root": "src"
    },
    "dist": {
        "path": "dist",
        "assets": {
            "path": "dist/assets",
            "scripts": "dist/assets/js",
            "styles": "dist/assets/css",
            "images": "dist/assets/img",
            "static": "dist/assets",
            "fontcustom": "dist/assets/fonts"
        },
        "medias": "dist/medias",
        "root": "dist"
    },
    "grunt": {
        "jshint": {
            "files": [
                "Gruntfile.js",
                "package.json",
                "happy-plan.json",
                "<%= happyPlan.src.assets.scripts %>/**/*.js",
                "!<%= happyPlan.src.assets.scripts %>/lib/**/*"
            ]
        }
    }
}
